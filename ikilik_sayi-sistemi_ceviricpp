#include <stdio.h>
 double to_bin(int sayi){
	int kalan,bsmk=1;
	double tmp=0;
	while(sayi>0){
		
		kalan=sayi%2;
		bsmk*=10;
		tmp+=kalan*bsmk;
		sayi/=2;
		
	}
return tmp;
	
}

int main(){
	int i;
	for(i=1; i<=300;i++){
		printf("%3d %15.0f\n",i,to_bin(i));
		}
	
	return 0;
}
/* 1'den 300'e kadar olan sayıların 2 ile kalanını hesaplayıp daha sonra ilgili basamakları 10 ile çarpar ve ikilik sayı sistemine göre yazdırır.
Sayı bitene kadar sürekli tekrarlar ve diğer sayıyı da aynı şekilde yapar */

